#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> v;
    for(int i = 1; i<1000; i++){
        if (i%3==0 || i%5==0){
            v.push_back(i);
        }
    }
    int sum = 0;
    for(int i = 0; i<v.size(); i++){
        sum += v[i];
    }
    cout<<sum;
    return 0;
}
